  "license": "MIT",
  "engines": {
    "node": ">= 18.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.350.0",
    "@aws-sdk/util-dynamodb": "^3.350.0",
    "@fastify/caching": "^8.3.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^10.1.1",
    "bootstrap": "^5.3.0",
    "convict": "^6.2.4",
    "fastify": "^4.18.0",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/convict": "^6.1.2",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "dotenv": "^16.1.4",
    "eslint": "^8.48.0",
